    "@types/turndown": "^5.0.5",
    "@wxt-dev/module-react": "^1.1.3",
    "shadcn": "^2.5.0",
    "tailwindcss": "^4.1.7",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/react-switch": "^1.2.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-tiktoken": "^1.0.20",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0",
